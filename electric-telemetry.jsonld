{
    "@context": [
        "https://www.w3.org/2022/wot/td/v1.1",
        {
            "@language": "en",
            "ditto": "https://ditto.eclipseprojects.io/wot/ditto-extension#",
            "om2": "http://www.ontology-of-units-of-measure.org/resource/om-2/",
            "time": "http://www.w3.org/2006/time#"
        }
    ],
    "@type": "tm:ThingModel",
    "title": "Electric Telemetry",
    "description": "Telemetry data of the Electricity Monitoring System",
    "securityDefinitions": {
        "nosec_sc": {
            "scheme": "nosec"
        }
    },
    "security": "nosec_sc",
    "properties": {
        "acqTime": {
            "type": "string",
            "description": "Acquisition time of the telemetry data",
            "readOnly": true,
            "title": "Acquisition Time",
            "@type": "time:inXSDDateTime"
        },
        "connTime": {
            "type": "string",
            "description": "publish time of the telemetry data",
            "readOnly": true,
            "title": "Connection Time",
            "@type": "time:inXSDDateTime"
        }
    }
}
